var assert = require("assert");

enum {
    zero = 0,
    one,
    two,
    three = 3,
    four,
    thousand = 1000,
    thousand_one
}


enum {
    negative_five = -5,
    negative_four,
    negative_three,
    negative_two
}


const the_answer = 42;


enum { one_line }
enum { extra_comma, }

function runTests()
{
    assert.equal(zero, 0);
    assert.equal(one,  1);
    assert.equal(two,  2);
    assert.equal(three, 3);
    assert.equal(four, 4);
    assert.equal(thousand, 1000);
    assert.equal(thousand_one, 1001);

    assert.equal(negative_five,  -5);
    assert.equal(negative_four,  -4);
    assert.equal(negative_three, -3);
    assert.equal(negative_two,   -2);

    assert.equal(one_line, 0);
    assert.equal(extra_comma, 0);

    assert.equal(the_answer, 42);

    return true;
}


runTests();
